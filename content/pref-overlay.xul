<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<?xml-stylesheet href="less://komodo/skin/global/global.less" type="text/css"?>

<prefwindow id="pof-prefs"
     title="Publish Open Files Preferences"
	 xmlns:html="http://www.w3.org/1999/xhtml"
	 width="400"
	 height="110"
	 hidechrome="true"
     xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
	
	<prefpane id="pof-pane" label="Publish Open Files" style="padding-top: 0;">
		<preferences>
			<preference id="pref_force" name="extensions.pof.forcePush" type="bool"/>
		</preferences>
		
		<titlebar id="pofTitleBar" height="30" width="100%"  style="background: transparent;">
			<label flex="1" style="text-align: center; padding: 7px 0; pointer-events: none;">
				Publish Open Files
			</label>
		</titlebar>
		
		<checkbox id="hideBtn" preference="pref_force" label="When a conflict occures, force push the conflicted file" />
		
	</prefpane>

</prefwindow>
